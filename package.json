{
  "name": "fetcher",
  "version": "10.0.0",
  "description": "a media downloader",
  "engines": {
    "node": ">=22.11.0"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d",
  "type": "module",
  "scripts": {
    "start-dev": "run-p -lr start-dev:*",
    "start-dev:frontend": "webpack --watch --progress",
    "start-dev:server": "tsx src/index.ts",
    "build": "pnpm clean && run-p --aggregate-output build:*",
    "build:ts": "tsc",
    "build:pug": "mkdir -p dist/views && cp -fr src/views dist",
    "build:frontend": "webpack --color",
    "lint": "run-p --aggregate-output --continue-on-error lint:*",
    "lint:eslint": "eslint src/**/*.{ts,js} *.config.cjs",
    "lint:prettier": "prettier --check 'src/**/*.{ts,js}'",
    "lint:stylelint": "stylelint --color 'src/**/*.css'",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "test": "NODE_ENV=test LOG_LEVEL=silent c8 mocha --require tsx --color 'src/**/*.spec.ts' --exit",
    "clean": "rm -rf .nyc_output/ dist/"
  },
  "dependencies": {
    "chalk": "5.6.0",
    "express": "4.21.2",
    "form-data": "4.0.4",
    "got": "14.5.0",
    "irc-colors": "1.5.0",
    "irc-framework": "4.14.0",
    "level": "8.0.1",
    "loglevel": "1.9.2",
    "loglevel-plugin-prefix": "0.8.4",
    "pug": "3.0.3",
    "rss-parser": "3.13.0",
    "source-map-support": "0.5.21",
    "tough-cookie": "6.0.0",
    "tslib": "2.8.1",
    "webtorrent": "2.8.4"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@types/chai": "5.2.2",
    "@types/express": "4.17.23",
    "@types/mocha": "10.0.10",
    "@types/node": "22.18.10",
    "@types/sinon": "17.0.4",
    "@types/stream-buffers": "3.0.7",
    "@types/xml2js": "0.4.14",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "c8": "10.1.3",
    "chai": "6.2.0",
    "css-loader": "7.1.2",
    "cssnano": "7.1.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.4.0",
    "jquery": "3.7.1",
    "mini-css-extract-plugin": "2.9.4",
    "mocha": "11.7.4",
    "mock-fs": "5.5.0",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "prettier": "3.6.2",
    "sinon": "21.0.0",
    "stream-buffers": "3.0.3",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "39.0.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1",
    "webpack-manifest-plugin": "5.0.1"
  }
}
